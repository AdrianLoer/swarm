  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "swarm-rdt": "^2.0.20",
    "swarm-ron": "^2.0.23",
    "swarm-clock": "^2.0.0",
    "swarm-ron-uuid": "^2.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "flow-bin": "^0.61.0",
    "flow-copy-source": "^1.2.1",
    "rimraf": "^2.6.2"
  },
  "files": [
    "*.js",
    "README.md"
  ],
  "scripts": {
    "test": "npm run build:lib && node ./test.js && echo OK",
    "build": "npm run build:clean && npm run build:lib && npm run build:flow",
