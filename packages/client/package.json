  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "reconnectable-websocket": "^0.1.10",
    "regenerator-runtime": "^0.11.1",
    "swarm-clock": "^2.0.0",
