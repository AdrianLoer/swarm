{
  "id": "peerAB",
  "connect": {
    "peerA": "tcp://localhost:1111"
  },
  "listen": [
    "ws://localhost:2222/dbname"
  ],
  "pub": [
    ".lww?depth=3",
    "rga",
    ".txt",
    ".json"
  ],
  "sub": {
    "peerA": ".lww#root?depth=3"
  },
  "mount": {
    "root.json": ".json#root?depth=2",
    "rga.txt": ".txt#rga"
  }
}